#include<stdio.h>
main()
{
int n,r,ecount=0,ocount=0;
printf("enter any number");
scanf("%d",&n);
while(n>0)
{
	r=n%10;
	if(r%2==0)
	ecount=ecount+1;
	else
	ocount=ocount+1;
	n=n/10;
}
if(ecount==ocount)
printf("Fancy Number");
else
printf("Not Fancy Number");
}

output:-
enter any number 123456
Fancy Number
--------------------------------
Process exited after 3.898 seconds with return value 0
Press any key to continue . . .
