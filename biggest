b=int(input("Enter value of b"))
c=int(input("Enter value of c"))
d=int(input("Enter value of d"))
if a>b:
    if a>c:
        if a>d:
            print("a is big")
elif b>c:
    if c>d:
        print("b is big")
elif c>d:
    print("c is big")
else:
    print("d is big")
        
        
